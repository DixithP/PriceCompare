<html>
<head>
	<title></title>
</head>
<body>
	<table>		      	
	   	<tr>
				<th>Item</th>
				<th>Amazon Price</th>
				
				</tr>
				<% @amazon.each do |item| %>
					<tr>					
						<td><%= item[0] %></td>
						<td><%= item[1] %></td>
						<% item[2].each do |item| %>
						<td><img src="<%= item %>"></td>
						<% end %>

					</tr>
				<% end %>
		</table>

		
		<table>		      	
	   	<tr>
				<th>Item</th>
				<th>Craigslist Price</th>
				
				</tr>
				<% @craigslist.each do |item| %>
					<tr>					
						<td><%= item[0] %></td>
						<td><%= item[1] %></td>
						
					</tr>
				<% end %>
		</table>


		<table>		      	
	   	<tr>
				<th>Item</th>
				<th>Walmart Price</th>
				
				</tr>
				<% @walmart.each do |item| %>
					<tr>					
						<td><%= item[0] %></td>
						<td><%= item[1] %></td>
						
					</tr>
				<% end %>
		</table>

		<table>		      	
	   	<tr>
				<th>Item</th>
				<th>Bestbuy Price</th>
				
				</tr>
				<% @bestbuy.each do |item| %>
					<tr>					
						<td><%= item[0] %></td>
						<td><%= item[1] %></td>
						
					</tr>
				<% end %>
		</table>


</body>
</html>